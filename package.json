{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "test": "vitest",
    "chat-server": "node server/chatServer.mjs",
    "dev:fullstack": "concurrently \"npm:chat-server\" \"npm:dev\"",
    "start": "concurrently \"npm run chat-server\" \"npm run preview -- --host\"",
    "generate:photos": "node scripts/generatePhotoPairs.mjs",
    "prepare": "husky"
  },
  "dependencies": {
    "@langchain/core": "^0.2.16",
    "@langchain/openai": "^0.0.18",
    "canvas-confetti": "^1.9.3",
    "chokidar": "^3.5.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "framer-motion": "^11.5.4",
    "gsap": "^3.12.5",
    "langchain": "^0.2.20",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use-gesture": "^9.1.3",
    "sharp": "^0.33.3",
    "tailwind-merge": "^3.4.0",
    "vite": "^7.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^13.24.0",
    "husky": "^9.1.7",
    "jsdom": "^23.0.1",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.47.0",
    "vitest": "^1.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
