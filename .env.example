# Frontend variables
VITE_FORCE_UNLOCK=false
ENV=DEV

# Phase 5: Generalized Products Feature Flags
VITE_ENABLE_GENERALIZED_PRODUCTS=false
VITE_ENABLE_PRODUCT_TYPE_CALENDAR=true
VITE_ENABLE_PRODUCT_TYPE_STORYBOOK=false
VITE_ENABLE_PRODUCT_TYPE_INTERACTIVE_GAME=false
VITE_ENABLE_TEMPLATE_SELECTION=false
VITE_ENABLE_CUSTOM_DATA_FORMS=false
VITE_ENABLE_PRODUCT_PREVIEW=false

# OpenAI (for intelligence service) - Set in .env.local (not committed)
# OPENAI_API_KEY=your-openai-api-key-here

# Advent Calendar API Environment Variables
NODE_ENV=development
PORT=3001

# Supabase Configuration (replace with real values)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
SUPABASE_PUBLISHABLE_KEY=your-publishable-key-here

# Intelligence Service
INTELLIGENCE_SERVICE_URL=http://localhost:8000

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Logging
LOG_LEVEL=info

# Optional: JWT Secret for custom tokens
# JWT_SECRET=your-jwt-secret-here